						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"dev_id\": \"001122334455\",\n    \"rff_score\": 0.85,\n    \"rff_confidence\": 0.9,\n    \"snr\": 20.0\n}"
						},
						"url": {
