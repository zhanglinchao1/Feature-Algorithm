	},
	"item": [
		{
			"name": "管理",
			"item": [
				{
					"name": "获取服务器状态",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/api/status",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"status"
							]
						},
						"description": "获取服务器当前状态，包括模式1和模式2的初始化情况及设备数量"
					},
					"response": []
				},
				{
					"name": "重置服务器状态",
					"request": {
						"method": "POST",
						"header": [],
						"url": {
							"raw": "{{base_url}}/api/reset",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"reset"
							]
						},
						"description": "重置所有服务器状态，清除已注册的设备和会话"
					},
					"response": []
				}
			]
		},
		{
			"name": "模式1 - RFF快速认证",
			"item": [
				{
					"name": "1. 注册设备",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"状态码为200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"注册成功\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.success).to.be.true;",
									"    pm.expect(jsonData.data.registered).to.be.true;",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"dev_id\": \"001122334455\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/device/mode1/register",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"device",
								"mode1",
								"register"
							]
						},
						"description": "在模式1中注册设备"
					},
					"response": []
				},
				{
					"name": "2. RFF认证 - 成功",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"状态码为200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"认证成功\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.success).to.be.true;",
									"    pm.expect(jsonData.data.authenticated).to.be.true;",
									"    pm.expect(jsonData.data.decision).to.eql(\"ACCEPT\");",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
